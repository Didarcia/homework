   "source": [
    "import sqlite3\n",
    "from sqlite3 import OperationalError\n",
    "\n",
    "conn = sqlite3.connect('CLASSICMODELS.db')\n",
    "c = conn.cursor()\n",
    "\n",
    "fd = open('classimodels2.sql', 'r')\n",
    "sqlFile = fd.read()\n",
    "fd.close()\n",
    "\n",
    "sqlCommands = sqlFile.split(';')\n",
    "\n",
    "for command in sqlCommands:\n",
    "    c.execute(command.strip())"
   ]
  }
